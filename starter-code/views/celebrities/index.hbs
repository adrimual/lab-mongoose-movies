<h2>Celebrities</h2>
<div>
    <a href="/celebrities/new">Create a new celebrity</a>
    <br>

    {{#each celebrity}}
    <a href="/celebrities/{{id}}">{{this.name}}</a>

    <form action="/celebrities{{id}}/delete" method="POST">
    <button type="submit">Delete</button>
    </form>
    {{/each}}
</div>